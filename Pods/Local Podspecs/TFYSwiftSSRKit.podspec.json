{
  "name": "TFYSwiftSSRKit",
  "version": "1.0.0",
  "summary": "A powerful iOS/macOS shadowsocks client library written in Swift",
  "description": "TFYSwiftSSRKit is a comprehensive shadowsocks client library for iOS and macOS.\nFeatures:\n* Multiple encryption methods support (OpenSSL & libsodium)\n* GeoIP based routing\n* TLS support\n* Automatic network monitoring and reconnection\n* Rule-based proxy configuration\n* Traffic statistics",
  "homepage": "https://github.com/13662049573/TFYSwiftSSRUilt",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "田风有": "420144542@qq.com"
  },
  "platforms": {
    "ios": "15.0",
    "osx": "12.0"
  },
  "swift_versions": "5.0",
  "source": {
    "git": "https://github.com/13662049573/TFYSwiftSSRUilt.git",
    "tag": "1.0.0"
  },
  "source_files": "Classes/**/*",
  "resource_bundles": {
    "TFYSwiftSSRKit": [
      "Resources/**/*"
    ]
  },
  "frameworks": "SystemConfiguration",
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": [
      "$(PODS_TARGET_SRCROOT)/shadowsocks/**/*",
      "$(PODS_TARGET_SRCROOT)/libsodium/**/*",
      "$(PODS_TARGET_SRCROOT)/openssl/**/*",
      "$(PODS_TARGET_SRCROOT)/libmaxminddb/**/*"
    ],
    "LIBRARY_SEARCH_PATHS": [
      "$(PODS_TARGET_SRCROOT)/shadowsocks/**/*",
      "$(PODS_TARGET_SRCROOT)/libsodium/**/*",
      "$(PODS_TARGET_SRCROOT)/openssl/**/*",
      "$(PODS_TARGET_SRCROOT)/libmaxminddb/**/*"
    ],
    "SWIFT_INCLUDE_PATHS": [
      "$(PODS_TARGET_SRCROOT)/shadowsocks/**/*",
      "$(PODS_TARGET_SRCROOT)/libsodium/**/*",
      "$(PODS_TARGET_SRCROOT)/openssl/**/*",
      "$(PODS_TARGET_SRCROOT)/libmaxminddb/**/*"
    ],
    "ENABLE_BITCODE": "NO"
  },
  "module_map": "module.modulemap",
  "swift_version": "5.0"
}
